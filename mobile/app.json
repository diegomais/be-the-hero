{
  "expo": {
    "name": "be-the-hero",
    "slug": "be-the-hero",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#e02041"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#e02041"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Be The Hero is a project that aims to connect people interested in helping NGOs with open campaigns.",
    "githubUrl": "https://github.com/diegomais/be-the-hero"
  }
}
